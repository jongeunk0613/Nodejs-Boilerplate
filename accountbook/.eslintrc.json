{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "parserOptions": {
      "ecmaVersion": 12
  },
  "rules": {
    "max-len": ["error", { "ignoreStrings": true }],
    "prettier/prettier": "error",
    "eol-last": 2
  },
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "ignorePatterns": ["*_bundle.js"]
}